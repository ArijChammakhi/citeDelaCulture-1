{% extends "base.html.twig" %}
{% block content %}
<html>
<body>
<h1>ajouter un evenment</h1>
<center>


    <div class="container">
        <div class="row">
            <!-- Contact form Section Start -->
            <div class="col-md-6">
                <h2>DÃ©posez un evenement   </h2>
                <form enctype="multipart/form-data" method="post" class="contact" >

                    <div class="form-group">
                        {{ form_widget(form.salle, {'attr': {'class': 'form-control'}}) }}
                    </div>
                    <div class="form-group">
                        {{ form_widget(form.prix, {'attr': {'class': 'form-control'}}) }}
                    </div>
                    <div class="form-group">
                        {{ form_widget(form.description, {'attr': {'class': 'form-control'}}) }}
                    </div>

                    <div class="form-group">
                        {{ form_widget(form.categorie, {'attr': {'class': 'form-control'}}) }}
                    </div>

                    <div class="form-group">
                        {{ form_widget(form.nom, {'attr': {'class': 'form-control'}}) }}
                    </div>
                    <div class="form-group">
                        {{ form_widget(form.dateebut, {'attr': {'class': 'form-control'}}) }}
                    </div>
                    <div class="form-group">
                        {{ form_widget(form.dateFin, {'attr': {'class': 'form-control'}}) }}
                    </div>

                    <div class="input-group">
                        {{ form_widget(form.ajouter, {'attr': {'class': 'form-control'}}) }}
                    </div>
                </form>
            </div>

        </div>
    </div>



</center>
</body>

</html>
{% endblock %}